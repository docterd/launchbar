<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.obrhoff.launchbar.cocoapods</string>
	<key>CFBundleIconFile</key>
	<string>Cocoapods</string>
	<key>CFBundleName</key>
	<string>CocoaPods</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	<key>LBSuggestionsScriptRunInBackground</key>
	<true/>
	<key>LBScripts</key>
	<dict>
		<key>LBDefaultScript</key>
		<dict>
			<key>LBScriptName</key>
			<string>default.py</string>
			<key>LBRunInBackground</key>
			<true/>
			<key>LBRequiresArgument</key>
			<true/>
			<key>LBAcceptedArgumentTypes</key>
			<array>
				<string>string</string>
			</array>
			<key>LBReturnsResult</key>
			<false/>
		</dict>
		<key>LBSuggestionsScript</key>
		<dict>
			<key>LBScriptName</key>
			<string>suggestions.py</string>
			<key>LBRunInBackground</key>
			<true/>
		</dict>
	</dict>
	<key>LBDescription</key>
	<dict>
		<key>LBSummary</key>
		<string>This action performs a Cocoapods search with the specified text. While typing, you get live feedback with suggestions according to the entered text.</string>
		<key>LBAuthor</key>
		<string>Dennis Oberhoff</string>
		<key>LBTwitter</key>
		<string>@launchbar</string>
	</dict>
</dict>
</plist>
